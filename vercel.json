{
  "rewrites": [
    { "source": "/((?!api/|_next/|static/|assets/|favicon.ico).*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*).(js|css|woff2|png|jpg|gif|svg|ico)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}